{
    "name": "csslayout",
    "scripts": {
        "dev": "parcel client/index.html --out-dir dist",
        "build": "parcel build client/index.html --out-dir dist",
        "postbuild": "react-snap"
    },
    "dependencies": {
        "highlight.js": "^9.16.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "react-snap": "^1.23.0"
    },
    "devDependencies": {
        "parcel-bundler": "^1.12.4"
    },
    "reactSnap": {
        "source": "dist",
        "minifyHtml": {
            "collapseWhitespace": false,
            "removeComments": false
        }
    }
}
